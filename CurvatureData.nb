  RowBox[{"metric", "=", 
   RowBox[{"(", "\[NoBreak]", GridBox[{
      {
       RowBox[{"-", 
        RowBox[{"f", "[", "r", "]"}]}], "0"},
      {"0", 
       RowBox[{"h", "[", "r", "]"}]}
     }], "\[NoBreak]", ")"}]}], ";"}], "\n", 
